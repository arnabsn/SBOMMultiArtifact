pipeline {
    agent any

    stages {
        stage('Build') {
            steps {
                echo 'build'
            }
        }
        stage('Security') {
            steps {
                snDevOpsSecurityResult securityResultAttributes: '{"scanner": "Veracode", "applicationName": "VeraDemo10"}'
            }
        }
        stage('Change') {
            steps {
                snDevOpsChange()
            }
        }
    }
}
